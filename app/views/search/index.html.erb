<h1>Search Results</h1>

<% @stations.each do |station| %>
  <div class="station">
    <ul>
      <li class="station_name"><%= station.name %></li>
      <li class="station_address"><%= station.address %></li>
      <li class="station_fuel_type"><%= station.fuel_types %></li>
      <li class="station_distance"><%= station.distance %></li>
      <li class="station_access_times"><%= station.access_times %></li>
    </ul>
  </div>
<% end %>
